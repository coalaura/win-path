{
	"name": "win-path",
	"displayName": "Windows Path",
	"description": "Escape windows path backslashes automatically on paste.",
	"version": "1.0.0",
	"author": "coalaura",
	"publisher": "coalaura",
	"icon": "logo.png",
	"galleryBanner": {
		"color": "#051049",
		"theme": "dark"
	},
	"license": "SEE LICENSE IN LICENSE",
	"keywords": [
		"paste",
		"path",
		"windows",
		"backslash"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/coalaura/win-path"
	},
	"engines": {
		"vscode": "^1.77.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"activationEvents": [
		"onWillChangeTextDocument",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"scripts": {
